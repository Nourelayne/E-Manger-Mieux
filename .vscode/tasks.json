{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "run-frontend",
      "type": "process",
      "command": "npm",
      "args": ["run", "dev"],
      "options": {
        "cwd": "${workspaceFolder}/Sources/front"
      },
      "isBackground": true
    },
    {
      "label": "build-backend",
      "type": "process",
      "command": "dotnet",
      "args": [
        "build",
        "${workspaceFolder}/Sources/bff/E-Manger-Mieux.Web/E-Manger-Mieux.Web.csproj"
      ],
      "problemMatcher": "$msCompile",
      "group": "build"
    }
  ]
}
